<div class ="modal taskmodal">
	<div class ="modal-contents">
		<form id ="addtaskform" method="post">

			<input type="text" id ="tasktitle" name ="title" placeholder ="Title"></input>
			<input type="text" id ="taskdescription" name ="description" placeholder ="Description"></input>
			<input type="text" id ="taskassociate" name ="associate" placeholder ="Associate with"></input>
			<input type ="date" id ="taskdeadline" name ="deadline" placeholder ="Deadline" min="2017-01-01"></input>
			<input type ="number" id ="taskimportance" name ="importance" placeholder ="Importance" min="1" max="5"></input>
			<input type="button" id ="addtask-submit" value="Submit">

		</form>

	</div>
</div>


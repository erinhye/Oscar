<div class = "contents list-container">

	<div class = "list list-dimension group">
		<div class = "group-title" id ="Unsorted">Unsorted</div>
		<ul class="list-inner-scroll">

			<%if(contents.length>0){%>
	        <%var i = 0;%>
	        <%contents.forEach(function(item){%>
	        <%i++;%>

					<%if(item.group == 'Unsorted' && item.deleted == false){%>
						<li class="list-item">
							<div class = "prioritized <%=item.importance%>"></div>
							<h2><%=item.dlday%>, <%=item.dlmonth%>, <%=item.dlyear%></h2>
							<br/>
							<h3><%=item.title%> With @<%=item.associate%></h3>
							<!-- <p>your descriptions are hereyour descriptions are here</p> -->

